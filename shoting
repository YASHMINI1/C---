#include<iostream>
using namespace std;
int main()
{
    int n,a[10],i,j,temp;
    cout<<"how many numbers you want";
    cin>>n;
    //accept numbers
    for(i=0;i<n;i++)
    {
     cin>>a[i];   
    }
    //display numbers
    for(i=0;i<n;i++)
    {
    for(j=0;j<n-i;j++)
    {
    if(a[j]>a[j+1])
    {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }

    for(j=0;j<=0;j++)
    {
     cout<<a[j];
    }
    return 0;
}
