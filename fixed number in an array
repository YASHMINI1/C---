#include<iostream>
using namespace std;
int main()
{
    int i,arr[20],j,n;
    cout<<"enter the size of an array: ";
    cin>>n;
    cout<<"enter the elements of the array: ";
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        if(arr[i]==i)
            cout<<arr[i];
    }
    return 0;
}
