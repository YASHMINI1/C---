#include<iostream>
using namespace std;
int main()
{
    int n,i,arr[20],flag;
    cout<<"enter the size of array: ";
    cin>>n;
    cout<<"enter the elements of the array: "<<endl;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cout<<"duplicate number is: ";
    for(i=0;i<n;i++)
    {
        if(arr[i]==arr[i+1])
            {
                cout<<arr[i];
            }
    }
    return 0;
}
