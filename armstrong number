#include<iostream>
using namespace std;
int main()
{
    int n,a,s,r;
    cout<<"enter the number";
    cin>>a;
    s=0;
    n=a;
    while(n>0)
    {
        r=n%10;
        s=s+(r*r*r*r);
        n=n/10;
    }
    if(s==a)
        cout<<"it is armstrong";
    else
        cout<<"not a armstrong";
    return 0;
}
