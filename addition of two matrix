#include<iostream>
using namespace std;
int main()
{
    int i,j;
    int a[10][10],b[10][10],c[10][10];
    cout<<"enter 1st matrix";
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            cin>>a[i][j];
        }
    }
     cout<<"enter 2nd matrix";
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            cin>>b[i][j];
        }
    }
    cout<<endl<<"1st matrix:"<<endl;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<endl<<"2nd matrix:"<<endl;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            cout<<b[i][j]<<" ";
        }
        cout<<endl;
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            {
                c[i][j] = a[i][j]+b[i][j];
            }
        }
    }
    cout<<endl<<"Addition matrix:"<<endl;
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            {
                cout<<c[i][j]<<" ";
            }
            cout<<endl;
        }
    }
    return 0;
}
