#include<iostream>
 using namespace std;

int main()
{
    int n,i,arr[100];
    int temp;
    cout<<"enter size of arrey:";
    cin>>n;
    cout<<"enter elements of arrey:";
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        cout<<arr[i];
    }
    temp=(n+1)*(n+2)/2;
    for(i=0;i<n;i++)
    {
        temp-=arr[i];
    }
    cout<<"missing number: "<<endl;
    cout<<temp;

}
