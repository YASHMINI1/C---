#include<iostream>
#include<string>
using namespace std;


int main()
{
    string s1,s2,s3;
    cout<<" Enter the first string: ";
    cin>>s1;
    cout<<"Enter the second string: ";
    cin>>s2;
    s3=s2;
    s2=s1;
    s1=s3;
    cout<<"string 1: "<<s1<<endl;
    cout<<"string 2: "<<s2<<endl;
    return 0;
}

