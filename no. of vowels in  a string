#include<iostream>
#include<string>
using namespace std;
int main()
{
    int count=0;
    string s="";
    cout<<"enter a string"<<endl;

    getline(cin,s);
    for(int i=0;i<s.length();i++)
    {
        if(s[i]==('a')||s[i]==('e')||s[i]==('i')||s[i]==('o')||s[i]==('u'))
        {
          count=count+1;
        }
    }
    cout<<count;
    return 0;
}
