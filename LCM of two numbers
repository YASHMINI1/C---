#include<iostream>
using namespace std;
int main()
{
    int m,n,i,x;
    cout<<"enter the first number : ";
    cin>>m;
    cout<<"enter the second number : ";
    cin>>n;
    x=m*n;
    cout<<"LCM of "<<m<<"and"<<n<<"is : ";
    for(i=1;i<=x;i++)
    {
        if(x%i==0 && i%m==0 && i%n==0)
        {
            cout<<i;
            break;
        }
    }
    return 0;
}
